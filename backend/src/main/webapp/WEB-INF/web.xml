<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">
    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>com.example.Noam.myapplication.backend.MyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>TumblerAppResourceServlet</servlet-name>
        <servlet-class>com.example.Noam.myapplication.backend.TumblerAppResourceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TumblerAppResourceServlet</servlet-name>
        <url-pattern>/app_res</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UploadPostServlet</servlet-name>
        <servlet-class>com.example.Noam.myapplication.backend.UploadPostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadPostServlet</servlet-name>
        <url-pattern>/upload_post</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>